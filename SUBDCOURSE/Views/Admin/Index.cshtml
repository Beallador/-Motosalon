@model IEnumerable<Moto>
       

    <div>
        <a href="/Admin/Create">Добавить</a>
        <a href="/Admin/Order">Заказы</a>
    </div>
     
    <table class="table">
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.Name</td>
                <td>
                    <a asp-action="Details" asp-controller="admin" asp-route-id="@item.Id">Подробнее</a> |
                    <a asp-action="Edit" asp-controller="admin" asp-route-id="@item.Id">Изменить</a> |
                    <a asp-action="Delete" asp-controller="admin" asp-route-id="@item.Id">Удалить</a>
                </td>
            </tr>
        }
    </table>
