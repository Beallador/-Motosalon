@model Moto

<h2>@Model.Name</h2>
<div>
    <dl class="dl-horizontal">
        <dt>Цена</dt>
        <dd>@Model.Price</dd>

        <dt>Короткое описание</dt>
        <dd>@Model.ShortDesc</dd>

        <dt>Полное Описание</dt>
        <dd>@Model.LongDesc</dd>

        <dt>Количество в мотосалоне</dt>
        <dd>@Model.Available</dd>
    </dl>
</div>
